{
  "name": "sinesaw",
  "devDependencies": {
    "browserify": "^5.9.1",
    "coffee-reactify": "^0.5.0",
    "coffeeify": "^0.7.0",
    "watchify": "^1.0.1"
  },
  "dependencies": {
    "immutable": "^2.0.4",
    "react": "^0.11.1",
    "react-raf-batching": "git+https://github.com/charlieschwabacher/react-raf-batching.git"
  },
  "scripts": {
    "bundle": "browserify app/assets/javascripts/index.js --debug --transform coffee-reactify --extension '.cjsx' --extension '.coffee' -o app/assets/javascripts/bundle.js",
    "watch-js": "watchify app/assets/javascripts/index.js --debug --transform coffee-reactify --extension '.cjsx' --extension '.coffee' -o app/assets/javascripts/bundle.js",
    "start": "npm run watch-js & rails s"
  }
}
